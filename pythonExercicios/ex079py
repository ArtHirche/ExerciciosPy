num = []
while True:
    n = int(input('Digite um número: '))
    if n not in num:
        num.append(n)
    else:
        print('Esse valor já está na lista!')
    restart = str(input('Deseja prosseguir? (S/N) '))
    if restart in 'Nn':
        break
num.sort()
print(f'Você digitou os valores: {num}')